{
  "name": "toritsin/math",
  "description": "PHP library to work with sets.",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "php",
    "math",
    "sets",
    "powerset",
    "cartesian"
  ],
  "authors": [
    {
      "name": "Nickolay Toritsin",
      "email": "toritsin@gmail.com"
    }
  ],
  "homepage": "https://github.com/toritsin/math",
  "require": {
    "php": ">=7.4"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.11.0",
    "phpstan/phpstan": "^1.8.4",
    "phpstan/phpstan-phpunit": "^1.1.1",
    "phpstan/phpstan-strict-rules": "^1.4.3",
    "phpunit/phpunit": "^9.5.24"
  },
  "autoload": {
    "psr-4": {
      "Toritsin\\Math\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Toritsin\\Math\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "infection/extension-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}